{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "fs:default",
    "dialog:default",
    "store:default",
    "fs:allow-home-read-recursive",
    "vector-store:allow-version",
    "vector-store:allow-test",
    {
      "identifier": "fs:allow-home-read-recursive",
      "allow": [
        {
          "path": "$HOME/*"
        }
      ]
    }
  ]
}